{
  "name" : "Imageinfo",
  "version" : "1.0.1",
  "description" : "Get image info for images, including EXIF data",
  "background" : { "scripts": ["background.js"] },
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications"
   ],
  "minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16" : "imageinfo-16.png",
    "48" : "imageinfo-48.png",
    "128" : "imageinfo-128.png"
  },
  
   "web_accessible_resources": [
    "imageinfo-48.png"
  ],
  "manifest_version": 2
}
