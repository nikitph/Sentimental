{
  "name" : "Sentimental",
  "version" : "1.0.1",
  "description" : "Get image info for images, including EXIF data",
  "background" : { "scripts": ["background.js"] },
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications"
   ],
  "minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16" : "Artboard.png",
    "48" : "Artboard@2x.png",
    "128" : "Artboard@3x.png"
  },
  "browser_action": {
    "default_icon": "Artboard.png"
  },
  
   "web_accessible_resources": [
    "Artboard@2x.png"
  ],
  "manifest_version": 2
}
